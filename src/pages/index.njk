{% block content %}
<article
    class='clip intro'
    style='
        --shape: 4% 3%, 85% 11%, 100% 72%, 24% 100%, 0 73%;
        --aspect-ratio: 5 / 4
    '
>
    <section class='inner'>
        <h1>
            Portfolio of
            <br>
            <em>Vadim&nbsp;Vaitenko</em>
        </h1>
        <div class='intro-text'>
            <p>Select case studies.</p>
            <p>Contact via&nbsp;<a href='mailto:vadim@vaitenko.com'>vadim@vaitenko.com</a></p>
        </div>
        <ul class='intro-links'>
            <li>
                <a href='https://dribbble.com/ichik'>Dribbble</a>
            </li>
            <li>
                <a href='https://github.com/ichik'>GitHub</a>
            </li>
            <li>
                <a href='./cv.pdf'>CV</a>
            </li>
        </ul>
    </section>
</article>
<article
    class='clip photo'
    style='
        --shape: 54% 4%, 71% 22%, 66% 48%, 92% 68%, 100% 96%, 3% 90%, 10% 70%, 34% 51%, 29% 33%, 32% 14%;
        --aspect-ratio: 1
    '
>
    <div class='placeholder'>
